<!-- Begin page content -->
    <div class="container">
      <div class="page-header">
        <h1>Hackathon UTC</h1>
      </div>
      <div class="bs-example" data-example-id="striped-table" data-ng-controller="projectCtrl as vm">
            <table class="table table-striped" >
              <thead>
                <tr>
                  <th>#</th>
                  <th>Project</th>
                  <th>Repository</th>
                  <th>Score</th>
                </tr>
              </thead>
              <tbody>
                <tr ng-repeat="project in vm.projects">
                  <th scope="row">{{ $index + 1 }}</th>
                  <td>{{ project.name  }}</td>
                  <td>{{ project.repository }}</td>
                  <td>{{ project.points }}</td>
                </tr>
              </tbody>
            </table>
  </div>
  
  
      <p>Use <a href="../sticky-footer-navbar">the sticky footer with a fixed navbar</a> if need be, too.</p>
    </div>

    <footer class="footer">
      <div class="container">
        <p class="text-muted">Place sticky footer content here.</p>
      </div>
    </footer>